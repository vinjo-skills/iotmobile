<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Allow cleartext traffic for your Raspberry Pi Pico's IP -->
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">192.168.4.1</domain> <!-- Replace with your Pico's IP -->
    </domain-config>
</network-security-config>